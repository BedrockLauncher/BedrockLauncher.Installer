<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">
    <!--#region Main Section-->

    <v:String x:Key="LanguageString">Português - Portugal</v:String>

    <!--#endregion-->

    <!--#region UI -->

    <v:String x:Key="Installer_Title">Assistente de Instalação do Minecraft Bedrock Launcher</v:String>

    <v:String x:Key="Installer_Back_Text">Voltar</v:String>
    <v:String x:Key="Installer_Next_Text">Próximo</v:String>
    <v:String x:Key="Installer_Cancel_Text">Cancelar</v:String>
    <v:String x:Key="Installer_Finish_Text">Finalizar</v:String>

    <v:String x:Key="Installer_WelcomePage_Title">Bem-vindo ao Assistente de Instalação do Minecraft Bedrock Launcher</v:String>
    <v:String x:Key="Installer_WelcomePage_Info_Text">Isto vai instalar o Minecraft Bedrock Launcher no teu computador.</v:String>
    <v:String x:Key="Installer_WelcomePage_Info2_Text">É recomendado que feche todas as outras aplicações antes de continuar.</v:String>
    <v:String x:Key="Installer_WelcomePage_Info3_Text">Clique em Próximo para continuar, ou em Cancelar para sair do assistente.</v:String>

    <v:String x:Key="Installer_LicenseAgreement_Title">Licença de Uso</v:String>
    <v:String x:Key="Installer_LicenseAgreement_Info_Text">Por favor, leia a seguinte licença de uso. Tens de aceitar os termos de</v:String>
    <v:String x:Key="Installer_LicenseAgreement_Info2_Text">uso antes de continuar com a instalação.</v:String>
    <v:String x:Key="Installer_LicenseAgreement_Accept_Text">Eu concordo.</v:String>
    <v:String x:Key="Installer_LicenseAgreement_DontAccept_Text">Eu não concordo.</v:String>

    <v:String x:Key="Installer_InstallType_Title">Escolher tipo de instalação</v:String>
    <v:String x:Key="Installer_InstallType_Info_Text">Selecione a versão do launcher que quer transferir</v:String>

    <v:String x:Key="Installer_InstallType_InstallLocation_BetaRadioButton_Text">Versão experiemental</v:String>
    <v:String x:Key="Installer_InstallType_InstallLocation_ReleaseRadioButton_Text">Versão oficial</v:String>

    <v:String x:Key="Installer_InstallLocation_Title">Escolher local de instalação</v:String>
    <v:String x:Key="Installer_InstallLocation_Info_Text">O assistente vai instalar o Minecraft Bedrock Launcher na seguinte pasta. Para instalar numa pasta diferente,</v:String>
    <v:String x:Key="Installer_InstallLocation_Info2_Text">clique em Procurar e selecione outra pasta.</v:String>

    <v:String x:Key="Installer_InstallLocation_DestinationFolder_Header">Pasta de destinatório</v:String>
    <v:String x:Key="Installer_InstallLocation_Browse_Text">PROCURAR</v:String>

    <v:String x:Key="Installer_InstallLocation_CreateDesktopIcon_Text">Criar um ícone no ambiente de trabalho</v:String>
    <v:String x:Key="Installer_InstallLocation_AddToStartMenu_Text">Adicionar ao menu de início</v:String>
    <v:String x:Key="Installer_InstallLocation_RegisterApp_Text">Registar programa</v:String>

    <v:String x:Key="Installer_InstallationProgress_LaunchOnExit_Text">Abrir programa ao fechar</v:String>
                                                                
    <v:String x:Key="Installer_InstallationProgress_FilesInUse">Alguns dos arquivos estão no momento a ser usados por:</v:String>

    <v:String x:Key="Installer_InstallationProgress_Title">A instalar</v:String>
    <v:String x:Key="Installer_InstallationProgress_Info_Text">Por favor, espere enquanto o assistente instala o Minecraft Bedrock Launcher no seu computador.</v:String>

