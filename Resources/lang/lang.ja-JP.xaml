    <v:String x:Key="Installer_Title">Minecraft Bedrock Launcherセットアップウィザード</v:String>

    <v:String x:Key="Installer_Back_Text">戻る</v:String>
    <v:String x:Key="Installer_Next_Text">次へ</v:String>
